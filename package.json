{
  "name": "payments",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:webpack": "nest build --webpack",
    "develop": "nodemon --watch ./src --ext '.ts' --exec 'yarn build && yarn start'",
    "start": "NODE_ENV=dev nest start",
    "start:sls": "NODE_ENV=dev LOG_LEVEL=debug serverless offline --host 0.0.0.0 start",
    "start:debug": "ts-node src/runtime/api-local.ts",
    "start:dev": "ts-node-dev src/runtime/api-local.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "tg-cc": "find iac -name .terragrunt-cache -type d -prune -exec rm -fr {} +",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.6",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@vendia/serverless-express": "^4.5.2",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "get-port": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "serverless-offline": "^8.5.0",
    "swagger-ui-express": "^4.3.0",
    "ts-node-dev": "^1.1.8",
    "winston": "^3.3.3",
    "yargs": "^17.3.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "serverless": "^2.69.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
